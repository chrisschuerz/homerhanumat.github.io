CompRand <- function(frame,sizes,groups) {
grp <- rep(groups,times=sizes)
grp <- sample(grp,size=length(grp),replace=F)
treat.grp <- factor(grp,levels=groups)
frame$treat.grp <- treat.grp
frame
}
